{
  "name": "hidden-vc-bot",
  "version": "1.0.0",
  "description": "Discordサーバー向け「隠しボイスチャンネル」自動管理BOT",
  "main": "src/hiddenVC.ts",
  "scripts": {
    "build": "npm install",
    "start": "ts-node --transpile-only src/hiddenVC.ts",
    "dev": "ts-node src/hiddenVC.ts"
  },
  "dependencies": {
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "log4js": "^6.9.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/express": "^4.17.17",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
